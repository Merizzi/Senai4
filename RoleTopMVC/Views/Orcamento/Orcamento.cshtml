<main>
        <div class="content-footer">
            <div class="content-icons">
                <form action="#">
                    <a class="titleC">Cadastro De Evento</a>
                    <div>
                        <a> Nome Do Dono</a>
                        <a> Nome Do Evento</a>
                    </div>
                    <div>
                        <input type="name" placeholder="Nome Dono" maxlength="20" minlength="3" name="nome" required/>
                        <input type="name" placeholder="Nome Evento" maxlength="25" minlength="4" name="nEvento" required/>
                    </div>
                </form>
            </div>
            <div class="content-address">
                <div class="content-address-info">
                    <form action="#">
                        <a> Email</a>
                        <input type="email" placeholder="Email" class="email" maxlength="20" minlength="3" name="email" required/>
                        <a> CPF/CNPJ</a>
                        <input type="text" placeholder="CPF/CNPJ "class="cpfinp" maxlength="11" minlength="11" name="cnpj" required >
                    </form>
                    <a> Data Evento</a>
                    <input type="date" placeholder=""  class="datainp" required name="data"/>
                    <a> Horario Evento</a>
                    <input type="time" placeholder=""  class="datainp" required name="horario"/>
                    <a> Descrição Evento</a>
                    <input type="text" placeholder=""  class="datainp" id="descrinp" required name="descricao"/>
                    <div class="content-senha">

                    <div class="content-sen">
                        <a>Celular</a> 
                    </div>
                    <div class="content-sen-inp">
                            <input type="text" placeholder="(11 94424-5786)"  class="senhainp" required/>

                    </div>
                    </div>
                    <div class="content-address-infob">
                        <button id="back-to-top-btn" onclick="window.location.href='eventosadm.html'"> Login </button>
                    </div>
                </div>
                
            </div>
        </div>       
        
    </main>